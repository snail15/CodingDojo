<div class="row">
    <div class="col-sm-1"></div>
    <div class="col-sm-10">
        <h3>Create Wedding</h3>
        <form action="create" method="POST">
            <div class="form-group">
                <label for="nameone">Wedder One</label>
                <input name="nameone" aria-describedby="userHelp" type="text" class="form-control" id="nameone">
            </div>
             <div class="form-group">
                <label for="nametwo">Wedder Two</label>
                <input name="nametwo" aria-describedby="PasswordHelp" class="form-control" id="nametwo" type="text">
            </div>
             <div class="form-group">
                <label for="date">Date</label>
                <input name="date" aria-describedby="PasswordHelp" class="form-control" id="date" type="text">
            </div>
             <div class="form-group">
                <label for="location">Location</label>
                <input name="location" aria-describedby="PasswordHelp" class="form-control" id="location" type="text">
            </div>
            <button type="submit" class="btn btn-primary">Create Wedding</button>
        </form>
    </div>
    <div class="col-sm-1"></div>
</div>